  "release_path": "/home/greg/Release/wpadverts",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@wordpress/element": "^2.20.3",
    "@wordpress/scripts": "^27.2.0",
    "autoprefixer": "^10.4.17",
    "grunt-checktextdomain": "^1.0.1",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-run": "^0.8.1",
    "grunt-wp-i18n": "^1.0.3",
    "postcss": "^8.4.35",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.4.1"
  },
  "scripts": {
    "start": "wp-scripts start --config blocks.webpack.js",
    "build": "wp-scripts build --config blocks.webpack.js",
    "build:list": "wp-scripts build blocks/list/src/index.js --output-path=blocks/list/build",
    "watch:build/list": "wp-scripts build blocks/list/src/index.js --output-path=blocks/list/build --watch --webpack--devtool=source-map",
    "postcss:watch": "postcss ./assets/css/tailwind.css -o compiled.css --watch",
    "start/list": "wp-scripts start blocks/list/src/index.js --output-path=blocks/list/build --webpack--devtool=source-map",
    "start/search": "wp-scripts start blocks/search/src/index.js --output-path=blocks/search/build --webpack--devtool=source-map",
    "start/details": "wp-scripts start blocks/details/src/index.js --output-path=blocks/details/build --webpack--devtool=source-map",
    "start/publish": "wp-scripts start blocks/publish/src/index.js --output-path=blocks/publish/build --webpack--devtool=source-map",
    "start/manage": "wp-scripts start blocks/manage/src/index.js --output-path=blocks/manage/build --webpack--devtool=source-map",
    "start/categories": "wp-scripts start blocks/categories/src/index.js --output-path=blocks/categories/build --webpack--devtool=source-map",
    "start/single-author": "wp-scripts start blocks/single-author/src/index.js --output-path=blocks/single-author/build --webpack--devtool=source-map",
    "start/single-contact": "wp-scripts start blocks/single-contact/src/index.js --output-path=blocks/single-contact/build --webpack--devtool=source-map",
    "start/single-gallery": "wp-scripts start blocks/single-gallery/src/index.js --output-path=blocks/single-gallery/build --webpack--devtool=source-map",
    "start/single-value": "wp-scripts start blocks/single-value/src/index.js --output-path=blocks/single-value/build --webpack--devtool=source-map",
    "start/single-data-table": "wp-scripts start blocks/single-data-table/src/index.js --output-path=blocks/single-data-table/build --webpack--devtool=source-map",
    "start/single-notifications": "wp-scripts start blocks/single-notifications/src/index.js --output-path=blocks/single-notifications/build --webpack--devtool=source-map",
    "build/list": "wp-scripts build blocks/list/src/index.js --output-path=blocks/list/build",
    "build/search": "wp-scripts build blocks/search/src/index.js --output-path=blocks/search/build",
    "build/details": "wp-scripts build blocks/details/src/index.js --output-path=blocks/details/build",
    "build/publish": "wp-scripts build blocks/publish/src/index.js --output-path=blocks/publish/build",
    "build/manage": "wp-scripts build blocks/manage/src/index.js --output-path=blocks/manage/build",
    "build/categories": "wp-scripts build blocks/categories/src/index.js --output-path=blocks/categories/build",
    "build/single-author": "wp-scripts build blocks/single-author/src/index.js --output-path=blocks/single-author/build",
    "build/single-contact": "wp-scripts build blocks/single-contact/src/index.js --output-path=blocks/single-contact/build",
    "build/single-gallery": "wp-scripts build blocks/single-gallery/src/index.js --output-path=blocks/single-gallery/build",
    "build/single-value": "wp-scripts build blocks/single-value/src/index.js --output-path=blocks/single-value/build",
    "build/single-data-table": "wp-scripts build blocks/single-data-table/src/index.js --output-path=blocks/single-data-table/build",
    "build/single-notifications": "wp-scripts build blocks/single-notifications/src/index.js --output-path=blocks/single-notifications/build",
    "build/css": "postcss ./assets/css/tailwind.css -o compiled.css",
    "build-all-single": "npm run build/single-notifications && npm run build/single-data-table && npm run build/single-value && npm run build/single-gallery && npm run build/single-contact && npm run build/single-author",
    "build-all": "npm run build/list && npm run build/search && npm run build/details && npm run build/publish && npm run build/manage && npm run build/categories && npm run build-all-single && npm run build/css",
    "r-list": "wp-scripts build blocks/list/src/index.js --output-path=blocks/list/build"
  },
  "dependencies": {
    "@wordpress/components": "^26.0.1",
    "caniuse-lite": "^1.0.30001588"
  }
